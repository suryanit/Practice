import java.util.*;

class LRUEvictionStrategy implements EvictionStrategy {
    
    private class Node {
        String key;
        Node prev, next;
        Node(String key) { this.key = key; }
    }

    private final Map<String, Node> nodeMap = new HashMap<>();
    private final Node head, tail;

    public LRUEvictionStrategy() {
        head = new Node("");
        tail = new Node("");
        head.next = tail;
        tail.prev = head;
    }

    @Override
    public void keyAccessed(String key) {
        if (nodeMap.containsKey(key)) {
            Node node = nodeMap.get(key);
            removeNode(node);
            addToTail(node);
        }
    }

    @Override
    public void keyAdded(String key) {
        if (nodeMap.containsKey(key)) {
            keyAccessed(key);
            return;
        }
        Node newNode = new Node(key);
        nodeMap.put(key, newNode);
        addToTail(newNode);
    }

    @Override
    public void keyRemoved(String key) {
        Node node = nodeMap.remove(key);
        if (node != null) {
            removeNode(node);
        }
    }

    @Override
    public String evict() {
        Node lruNode = head.next;
        if (lruNode == tail) return null;

        String keyToEvict = lruNode.key;
        removeNode(lruNode);
        nodeMap.remove(keyToEvict);
        return keyToEvict;
    }

    private void removeNode(Node node) {
        node.prev.next = node.next;
        node.next.prev = node.prev;
    }

    private void addToTail(Node node) {
        node.prev = tail.prev;
        node.next = tail;
        tail.prev.next = node;
        tail.prev = node;
    }
}


class CacheBuilder {
    private final int capacity;
    private final Map<String, String> cacheMap = new HashMap<>();
    private final EvictionStrategy strategy;

    public CacheBuilder(int capacity, String strategyType) {
        this.capacity = capacity;
        if ("LRU".equalsIgnoreCase(strategyType)) {
            this.strategy = new LRUEvictionStrategy();
        } else {
            throw new IllegalArgumentException("Unsupported strategy: " + strategyType);
        }
    }

    public String get(String key) {
        if (!cacheMap.containsKey(key)) return null;
        
        strategy.keyAccessed(key);
        return cacheMap.get(key);
    }

    public void put(String key, String value) {
        if (cacheMap.containsKey(key)) {
            cacheMap.put(key, value);
            strategy.keyAccessed(key);
            return;
        }

        if (cacheMap.size() >= capacity) {
            String victimKey = strategy.evict();
            if (victimKey != null) {
                cacheMap.remove(victimKey);
            }
        }

        cacheMap.put(key, value);
        strategy.keyAdded(key);
    }

    public void delete(String key) {
        if (cacheMap.containsKey(key)) {
            cacheMap.remove(key);
            strategy.keyRemoved(key);
        }
    }
}
